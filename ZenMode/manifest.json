{
  "manifest_version":3,
  "name":"zen mode styler",
  "description":"this app let's you toggles to Dark Mode, Sepia Mode, or Hide Images mode on any website to make reading easier",
  "version":"1.0",
   "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "action":{
    "default_popup":"popup.html",
    "default_icon":{
      "16": "images/icon-16.png",
      "32": "images/icon-32.png"
    },
    "default_title":"Zen Mode Styler"
  },
  "permissions": [
  "activeTab",
  "scripting"
],
"host_permissions": [
  "https://*/*",
  "http://*/*"
]
}